pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
          bat 'mvn clean install sonar:sonar -Dsonar.sources=src sonar.nodejs.executable=./Program Files/nodejs/node.exe'          
        }
      }
    }
  }
}
